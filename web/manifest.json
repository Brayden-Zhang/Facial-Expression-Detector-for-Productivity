{
  "manifest_version":2,
  "version":"1.0",
  "name":"Test",
  "content_scripts":[
    {
     "matches":["<all_urls>"], 
     "js":["extensionTest.js"]
    }
  ],
  "icons": {
    "512": "images/test-icon.png"
  },
  "background":{
    "scripts": ["background.js"]
  },
  "browser_action":{
    "default_icon":{
      "512": "images/test-icon-toolbar.png"
    },
    "default_popup": "index.html"
   },
   "permissions": ["storage", "unlimitedStorage", "activeTab", "tabs"]
}